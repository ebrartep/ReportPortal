low-auto\">\n\n    <b-col lg=\"3\" class=\"my-1\" style=\"float: right\">\n      <b-form-group\n        label-for=\"filter-input\"\n        label-align-sm=\"right\"\n        label-size=\"sm\"\n      >\n        <b-input-group size=\"sm\">\n          <b-form-input\n            style=\"color: black !important\"\n            id=\"filter-input\"\n            v-model=\"filter\"\n            type=\"search\"\n            placeholder=\"Arama yapınız...\"\n          ></b-form-input>\n\n          <b-input-group-append>\n            <b-button\n              style=\"background-color: #6a60e7 !important\"\n              :disabled=\"!filter\"\n              @click=\"filter = ''\"\n              >Temizle</b-button\n            >\n          </b-input-group-append>\n        </b-input-group>\n      </b-form-group>\n    </b-col>\n\n     <select v-model=\"selected\" @change=\"onChange($event)\"  variant=\"gradient-primary\"  label=\"Arama yapınız...\">\n      <option  value=\"\">Seçiniz</option>\n\n      <option\n        v-for=\"(item, index) in filterItems\"\n        :key=\"index\"\n        :value=\"item.id\"\n      >\n        {{ item.companyName }}\n      </option>\n    </select>\n    <span>Seçilen: {{ selected }}</span>\n\n    <b-table\n      id=\"my-table\"\n      :items=\"items\"\n      :filter=\"filter\"\n      :fields=\"fields\"\n      :filter-included-fields=\"filterOn\"\n      :per-page=\"perPage\"\n      :current-page=\"currentPage\"\n      striped\n      responsive\n    >\n      <template #cell(show_details)=\"row\">\n        <b-form-checkbox\n          v-model=\"row.detailsShowing\"\n          plain\n          class=\"vs-checkbox-con\"\n          @change=\"row.toggleDetails\"\n        >\n          <span class=\"vs-checkbox\">\n            <span class=\"vs-checkbox--check\">\n              <i class=\"vs-icon feather icon-check\" />\n            </span>\n          </span>\n          <span class=\"vs-label\">{{\n            row.detailsShowing ? \"Hide\" : \"Show\"\n          }}</span>\n        </b-form-checkbox>\n      </template>\n\n      <template #row-details=\"row\">\n        <b-card>\n          <b-row class=\"mb-2\">\n            <b-col md=\"4\" class=\"mb-1\">\n              <strong>Name : </strong>{{ row.item.name }}\n            </b-col>\n\n            <b-col md=\"4\" class=\"mb-1\">\n              <strong>Company Name : </strong>{{ row.item.companyName }}\n            </b-col>\n            <b-col md=\"4\" class=\"mb-1\">\n              <strong>City : </strong>{{ row.item.city }}\n            </b-col>\n          </b-row>\n\n          <b-button\n            size=\"sm\"\n            variant=\"outline-secondary\"\n            @click=\"row.toggleDetails\"\n          >\n            Hide Details\n          </b-button>\n        </b-card>\n      </template>\n\n      <template #cell(avatar)=\"data\">\n        <b-avatar :src=\"data.value\" />\n      </template>\n\n      <template #cell(status)=\"data\">\n        <b-badge :variant=\"status[1][data.value]\">\n          {{ status[0][data.value] }}\n        </b-badge>\n      </template>\n    </b-table>\n\n    <b-pagination\n      v-model=\"currentPage\"\n      :total-rows=\"rows\"\n      size=\"ml\"\n      style=\"float: right\"\n    ></b-pagination>\n  </div>\n</template>\n\n<script>\nimport {\n  BTable,\n  BFormCheckbox,\n  BButton,\n  BCard,\n  BRow,\n  BCol,\n  BAvatar,\n  BBadge,\n  BPagination,\n  BFormGroup,\n  BFormInput,\n  BInputGroupAppend,\n  BInputGroup,\n} from \"bootstrap-vue\";\nimport vSelect from \"vue-select\";\n\nexport default {\n  components: {\n    \"b-form-group\": BFormGroup,\n    \"b-input-group\": BInputGroup,\n    \"b-form-input\": BFormInput,\n    \"b-input-group-append\": BInputGroupAppend,\n\n    vSelect,\n    BTable,\n    BButton,\n    BFormCheckbox,\n    BCard,\n    BRow,\n    BCol,\n    BBadge,\n    BAvatar,\n    BPagination,\n  },\n\n  data() {\n    return {\n      url: \"https://6053138445e4b30017290bdc.mockapi.io\",\n      rows: 100,\n      perPage: 10,\n      currentPage: 1,\n      filter: null,\n      filterOn: [],\n      filterByFormatted: true,\n      fields: [\"show_details\", \"id\", \"name\", \"companyName\", \"city\"],\n      items: [],\n      filterItems: [],\n      status: [\n        {\n          1: \"Current\",\n          2: \"Professional\",\n          3: \"Rejected\",\n          4: \"Resigned\",\n          5: \"Applied\",\n        },\n        {\n          1: \"light-primary\",\n          2: \"light-success\",\n          3: \"light-danger\",\n          4: \"light-warning\",\n          5: \"light-info\",\n        },\n      ],\n      selected: undefined,\n      dropDown: [],\n    };\n  },\n  created() {\n    let self = this;\n    this.$axios\n      .get(self.url + \"/UserApp/users\")\n      .then((res) => {\n        self.items = res.data;\n        self.names = res.data;\n        self.filterItems = res.data;\n      });\n  },\n  computed: {\n    rows() {\n      return this.items.length;\n    },\n    filteredItems() {\n      return this.items.filter((item) => {\n        let keep = true;\n\n        // This is a basic equality filter. What I did in the actual code was to have an object with filter functions for each key. If a key was missing, it defaulted to straight equality.\n        this.fieldKeys.forEach((key) => {\n          keep =\n            keep &&\n            (this.selectVal[key] === undefined ||\n              item[key] === this.selectVal[key]);\n        });\n\n        return keep;\n      });\n    },\n\n    fieldKeys() {\n      return Object.keys(this.items[0]);\n    },\n  },\n\n  methods: {\n    details(item) {\n      alert(JSON.stringify(item));\n    },\n    onChange(event) {\n      console.log(event.target.value);\n      if (event.target.value === \"\") {\n        let self = this;\n        this.$axios\n          .get(\"https://6053138445e4b30017290bdc.mockapi.io/UserApp/users\")\n          .then((res) => {\n            self.items = res.data;\n            self.filterItems = res.data;\n          });\n      } else {\n        let self = this;\n        let url = \"https://6053138445e4b30017290bdc.mockapi.io/UserApp/users/\";\n        this.$axios\n          .get(url + event.target.value)\n          .then(function (response) {\n            self.items = [];\n            self.items.push(response.data);\n\n            console.log(self.items);\n\n            console.log(response);\n          })\n          .catch(function (error) {\n            // handle error\n            console.log(error);\n          })\n          .then(function () {\n            // always executed\n          });\n      }\n    },\n  },\n};\n</script>\n<style lang=\"scss\">\n@import \"@core/scss/vue/libs/vue-select.scss\";\n</style>"],"sourceRoot":"src/views"}]}